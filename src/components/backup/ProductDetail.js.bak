// src/pages/ProductDetail.jsx
import React, { useEffect, useState, useContext, useRef } from "react";
import { useParams, Link, useNavigate } from "react-router-dom";
import axios from "axios";
import styled from "styled-components";
import { motion, AnimatePresence } from "framer-motion";
import { CartContext } from "../context/CartContext";
import PRODUCTS from "../data/products";

/* ---------------------------
  Local helpers / storage keys
---------------------------- */
const RV_KEY = "recently_viewed_v1";
const REVIEWS_KEY = "product_reviews_v1";
const WISH_KEY = "wishlist_v1";

function addRecentlyViewed(product) {
  try {
    if (!product || (product.id === undefined || product.id === null)) return;
    const raw = localStorage.getItem(RV_KEY);
    const list = raw ? JSON.parse(raw) : [];
    const cleaned = list.filter((p) => String(p.id) !== String(product.id));
    const entry = {
      id: product.id,
      title: product.title,
      image: product.image || product.img || "",
      price: product.price,
    };
    cleaned.unshift(entry);
    localStorage.setItem(RV_KEY, JSON.stringify(cleaned.slice(0, 24)));
  } catch (e) {
    console.error(e);
  }
}
function readRecentlyViewed() {
  try {
    const raw = localStorage.getItem(RV_KEY);
    return raw ? JSON.parse(raw) : [];
  } catch {
    return [];
  }
}
function readReviews(productId) {
  try {
    const raw = localStorage.getItem(REVIEWS_KEY) || "{}";
    const all = JSON.parse(raw);
    return all[String(productId)] || [];
  } catch {
    return [];
  }
}
function saveReview(productId, review) {
  try {
    const raw = localStorage.getItem(REVIEWS_KEY) || "{}";
    const all = JSON.parse(raw);
    all[String(productId)] = all[String(productId)] || [];
    all[String(productId)].unshift(review);
    all[String(productId)] = all[String(productId)].slice(0, 200);
    localStorage.setItem(REVIEWS_KEY, JSON.stringify(all));
  } catch (e) {
    console.error(e);
  }
}
function readWishlist() {
  try {
    const raw = localStorage.getItem(WISH_KEY);
    return raw ? JSON.parse(raw) : [];
  } catch {
    return [];
  }
}
function saveWishlist(list) {
  try {
    localStorage.setItem(WISH_KEY, JSON.stringify(list));
  } catch {}
}

/* ---------------------------
  Styled components (kept compact)
---------------------------- */
const PageWrapper = styled.div` width:100%; overflow-x:hidden; box-sizing:border-box; padding-bottom:120px; `;
const Container = styled.div`
  display:flex; flex-wrap:wrap; gap:40px; padding:24px 20px; justify-content:center; max-width:1200px; margin:0 auto;
  @media (max-width:768px) { gap:20px; padding:16px 12px; flex-direction:column; }
`;
const ImageWrap = styled.div` flex:0 0 380px; display:flex; flex-direction:column; gap:12px; align-items:center; `;
const MainImage = styled.img`
  width:100%; max-width:380px; height:380px; object-fit:contain; border-radius:12px; background:#fff; padding:12px; box-shadow:0 10px 30px rgba(15,23,42,0.06);
`;
const ThumbRow = styled.div` display:flex; gap:8px; overflow-x:auto; width:100%; justify-content:center; padding:6px; `;
const Thumb = styled.img` width:64px; height:64px; object-fit:cover; border-radius:8px; border:2px solid ${(p)=>p.active?"#1e1e2f":"transparent"}; cursor:pointer; `;
const Info = styled.div` flex:1 1 520px; max-width:720px; `;
const Title = styled.h1` font-size:22px; font-weight:700; margin:6px 0 12px; `;
const Price = styled.p` font-weight:700; font-size:1.4rem; margin:6px 0 12px; color:#0b74de; `;
const Stock = styled.div` font-size:13px; margin-bottom:10px; color:${(p)=>p.inStock?"#059669":"#ef4444"}; `;
const Desc = styled.p` color:#444; line-height:1.5; margin-bottom:12px; `;
const Controls = styled.div` display:flex; gap:12px; align-items:center; margin-top:12px; flex-wrap:wrap; `;
const QtyBox = styled.div` display:inline-flex; align-items:center; border-radius:8px; border:1px solid #e5e7eb; overflow:hidden; `;
const QtyBtn = styled.button` padding:8px 12px; background:#fff; border:0; cursor:pointer; `;
const QtyVal = styled.div` min-width:48px; text-align:center; font-weight:600; `;
const VariantWrap = styled.div` margin-top:12px; display:flex; gap:8px; align-items:center; flex-wrap:wrap; `;
const ColorSwatch = styled(motion.div)` width:34px; height:34px; border-radius:50%; border:2px solid ${(p)=>p.active?"#000":"#ddd"}; background-color:${(p)=>p.color}; cursor:pointer; `;
const SizeBtn = styled.button` padding:8px 10px; border-radius:8px; border:1px solid ${(p)=>p.active?"#111827":"#e5e7eb"}; background:${(p)=>p.active?"#111827":"#fff"}; color:${(p)=>p.active?"#fff":"#111827"}; cursor:pointer; `;
const AddCartButton = styled(motion.button)` margin-top:8px; padding:12px 18px; background:#111827; color:#fff; border:none; cursor:pointer; border-radius:8px; font-weight:600; `;
const SelectedInfo = styled.div` margin-top:12px; font-size:14px; color:#333; `;
const Toast = styled(motion.div)` position:fixed; right:20px; bottom:20px; background:#111827; color:white; padding:12px 16px; border-radius:8px; box-shadow:0 10px 30px rgba(2,6,23,0.2); z-index:99999; `;
const ReviewsWrap = styled.div` margin-top:18px; background:#f8fafc; padding:12px; border-radius:8px; `;
const RelatedGrid = styled.div` display:grid; grid-template-columns:repeat(auto-fit,minmax(160px,1fr)); gap:12px; margin-top:12px; `;
const RelatedCard = styled(Link)` display:block; text-decoration:none; background:#fff; padding:8px; border-radius:8px; box-shadow:0 6px 18px rgba(2,6,23,0.06); color:inherit; `;
const RelatedImg = styled.img` width:100%; height:120px; object-fit:cover; border-radius:6px; `;
const RVList = styled.div` display:flex; gap:12px; overflow-x:auto; padding-bottom:12px; & > a{ display:block; min-width:160px; background:#fff; border-radius:8px; padding:8px; box-shadow:0 6px 18px rgba(2,6,23,0.06); text-decoration:none; color:inherit; } img{ width:100%; height:96px; object-fit:cover; border-radius:6px; } `;
const StickyBar = styled.div` position:fixed; left:0; right:0; bottom:0; padding:10px 14px; background:rgba(255,255,255,0.98); box-shadow:0 -6px 18px rgba(2,6,23,0.06); display:flex; gap:8px; align-items:center; justify-content:center; z-index:120000; border-top:1px solid #eee; `;

/* ---------------------------
  Component
---------------------------- */
const ProductDetail = () => {
  // ---------- Hooks (all declared at top)
  const { addToCart } = useContext(CartContext);
  const navigate = useNavigate();
  const { id: rawId } = useParams();

  const [product, setProduct] = useState(null);
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState(null);

  const colors = ["#000000", "#ff0000", "#1e90ff", "#008000"];
  const sizes = ["S", "M", "L", "XL"];
  const [selectedColor, setSelectedColor] = useState(null);
  const [selectedSize, setSelectedSize] = useState(null);
  const [qty, setQty] = useState(1);

  const [mainImg, setMainImg] = useState(null);
  const [thumbs, setThumbs] = useState([]);

  const [toast, setToast] = useState(null);
  const [zoomOpen, setZoomOpen] = useState(false);
  const [reviews, setReviews] = useState([]);
  const [reviewForm, setReviewForm] = useState({ name: "", rating: 5, text: "" });

  const [submittingReview, setSubmittingReview] = useState(false);
  const [adding, setAdding] = useState(false);

  const [wishlist, setWishlist] = useState(readWishlist());
  const [showSticky, setShowSticky] = useState(false);

  const [reviewPageSize, setReviewPageSize] = useState(5); // pagination: show first N reviews
  const nameInputRef = useRef(null);

  const productId = React.useMemo(() => {
    if (!rawId) return null;
    const match = String(rawId).match(/(\d+)$/);
    return match ? match[1] : rawId;
  }, [rawId]);

  // ---------- Effects
  useEffect(() => {
    let mounted = true;
    setLoading(true);
    setError(null);
    try { window.scrollTo(0, 0); } catch {}

    (async () => {
      try {
        if (productId) {
          const local = PRODUCTS.find((p) => String(p.id) === String(productId));
          if (local) {
            if (!mounted) return;
            setProduct(local);
            setThumbs(normalizeImages(local));
            setMainImg(local.image || local.img || (local.images?.[0] ?? null));
            addRecentlyViewed(local);
            setReviews(readReviews(productId));
            setLoading(false);
            return;
          }
        }

        if (productId && /^\d+$/.test(String(productId))) {
          const res = await axios.get(`https://fakestoreapi.com/products/${productId}`);
          if (!mounted) return;
          if (res && res.data) {
            setProduct(res.data);
            setThumbs(normalizeImages(res.data));
            setMainImg(res.data.image || res.data.img || null);
            addRecentlyViewed(res.data);
            setReviews(readReviews(productId));
            setLoading(false);
            return;
          }
        }

        if (mounted) {
          setError("Product not found");
          setLoading(false);
        }
      } catch (err) {
        console.error(err);
        const local = PRODUCTS.find((p) => String(p.id) === String(productId));
        if (local && mounted) {
          setProduct(local);
          setThumbs(normalizeImages(local));
          setMainImg(local.image || local.img || null);
          addRecentlyViewed(local);
          setReviews(readReviews(productId));
          setLoading(false);
          return;
        }
        if (mounted) {
          setError("Failed to load product");
          setLoading(false);
        }
      }
    })();

    return () => { mounted = false; };
  }, [productId]);

  useEffect(() => {
    setSelectedColor(null);
    setSelectedSize(null);
    setQty(1);
    setReviewForm({ name: "", rating: 5, text: "" });
  }, [productId]);

  useEffect(() => {
    const onScroll = () => {
      const y = window.scrollY || window.pageYOffset;
      setShowSticky(y > 420);
    };
    window.addEventListener("scroll", onScroll, { passive: true });
    onScroll();
    return () => window.removeEventListener("scroll", onScroll);
  }, []);

  // keyboard shortcut (press "a" to add to cart) - avoid when typing
  useEffect(() => {
    const onKey = (e) => {
      if (e.key === "a" && document.activeElement && ['INPUT','TEXTAREA','SELECT'].includes(document.activeElement.tagName) === false) {
        doAddToCart();
      }
    };
    window.addEventListener("keydown", onKey);
    return () => window.removeEventListener("keydown", onKey);
  }, [selectedColor, selectedSize, qty, product]);

  // ---------- helpers (no hooks)
  function normalizeImages(prod) {
    if (!prod) return [];
    if (prod.images && Array.isArray(prod.images) && prod.images.length) return prod.images;
    const base = prod.image || prod.img || null;
    if (!base) return [];
    return [base, base + "?v=1", base + "?v=2"];
  }

  // wishlist toggling (persist)
  const toggleWishlist = () => {
    const cur = readWishlist();
    const exists = cur.find(i => String(i.id) === String(product.id));
    let next;
    if (exists) {
      next = cur.filter(i => String(i.id) !== String(product.id));
      setToast({ type: 'warn', text: 'Removed from wishlist' });
    } else {
      next = [{ id: product.id, title: product.title, image: product.image || product.img }, ...cur];
      setToast({ type: 'ok', text: 'Added to wishlist' });
    }
    saveWishlist(next);
    setWishlist(next);
    setTimeout(()=>setToast(null), 1800);
  };

  const inWishlist = () => wishlist.find(i => String(i.id) === String(product?.id));

  const copyLink = async () => {
    try {
      await navigator.clipboard.writeText(window.location.href);
      setToast({ type: "ok", text: "Link copied" });
      setTimeout(()=>setToast(null), 1500);
    } catch {
      setToast({ type: "warn", text: "Unable to copy" });
      setTimeout(()=>setToast(null), 1500);
    }
  };

  const onShareWhatsApp = () => {
    const text = `${product.title} - ₹${product.price}\n${window.location.href}`;
    const url = `https://wa.me/?text=${encodeURIComponent(text)}`;
    window.open(url, "_blank");
  };

  const onShare = async () => {
    const url = window.location.href;
    if (navigator.share) {
      try { await navigator.share({ title: product.title, text: product.description, url }); } catch {}
    } else {
      copyLink();
    }
  };

  const doAddToCart = async () => {
    if (adding) return;
    if (!selectedColor || !selectedSize) {
      setToast({ type: "warn", text: "Select color & size" });
      setTimeout(()=>setToast(null), 1600);
      return;
    }
    setAdding(true);
    const cartItem = { id: product.id, title: product.title, price: product.price, color: selectedColor, size: selectedSize, image: product.image || product.img, quantity: qty };
    try { addToCart(cartItem, qty); } catch { try { addToCart(cartItem); } catch (e) { console.error(e);} }
    setToast({ type: "ok", text: `Added ${qty} × ${product.title}` });
    setTimeout(()=>{ setToast(null); setAdding(false); }, 1100);
  };

  const handleAddToCart = () => doAddToCart();

  const increment = () => setQty(q => Math.min((product.stock || 99), q + 1));
  const decrement = () => setQty(q => Math.max(1, q - 1));

  const submitReview = (e) => {
    e.preventDefault();
    if (submittingReview) return;
    if (!reviewForm.name || !reviewForm.text) {
      setToast({ type: "warn", text: "Please enter name & review" });
      setTimeout(()=>setToast(null), 1500);
      return;
    }
    setSubmittingReview(true);
    const r = { id: Date.now(), name: reviewForm.name, rating: Number(reviewForm.rating), text: reviewForm.text, date: new Date().toISOString() };
    saveReview(productId, r);
    setReviews(prev => [r, ...prev]);
    setReviewForm({ name: "", rating: 5, text: "" });
    setTimeout(() => {
      setSubmittingReview(false);
      setToast({ type: "ok", text: "Thanks for your review" });
      setTimeout(()=>setToast(null), 1400);
    }, 700);
    // focus name input for accessibility after reset
    try { nameInputRef.current && nameInputRef.current.focus(); } catch {}
  };

  const resetReviewForm = () => {
    setReviewForm({ name: "", rating: 5, text: "" });
    try { nameInputRef.current && nameInputRef.current.focus(); } catch {}
  };

  // Show only page size reviews, user can click load more
  const displayedReviews = reviews.slice(0, reviewPageSize);

  // ---------- guards (render safe)
  if (loading) return <p style={{ padding: 20 }}>Loading...</p>;
  if (error) return <p style={{ padding: 20 }}>{error}</p>;
  if (!product) return <p style={{ padding: 20 }}>Product not found.</p>;

  const inStock = product.stock === undefined ? true : Boolean(product.stock > 0);
  const avgRating = reviews.length ? (reviews.reduce((s,r)=>s + (Number(r.rating)||0), 0) / reviews.length).toFixed(1) : null;
  const related = PRODUCTS.filter(p => p.category === product.category && String(p.id) !== String(product.id)).slice(0, 8);
  const recent = readRecentlyViewed().filter(r => String(r.id) !== String(product.id)).slice(0, 8);

  return (
    <PageWrapper>
      <Container>
        <ImageWrap>
          <div style={{ width: "100%", textAlign: "left", fontSize: 13, color: "#6b7280" }}>
            <div><Link to="/">Home</Link> › <Link to={`/shop?cat=${encodeURIComponent(product.category||"")}`}>{product.category||"Products"}</Link> › <span>{product.title}</span></div>
          </div>

          <div style={{ cursor: "zoom-in" }} onClick={() => setZoomOpen(true)}>
            <MainImage src={mainImg || product.image || product.img} alt={product.title} onError={(e)=>e.target.src = product.image || product.img || ""} />
          </div>

          <ThumbRow>
            {thumbs.map((t,i)=>(
              <Thumb key={i} src={t} alt={`thumb-${i}`} active={t===mainImg} onClick={()=>setMainImg(t)} />
            ))}
          </ThumbRow>

          <div style={{ display: "flex", gap: 8 }}>
            <button onClick={onShare} style={{ padding: "8px 10px", borderRadius: 8, border: "1px solid #e5e7eb", background:"#fff", cursor:"pointer" }}>Share</button>
            <button onClick={copyLink} style={{ padding: "8px 10px", borderRadius: 8, border: "1px solid #e5e7eb", background:"#fff", cursor:"pointer" }}>Copy link</button>
            <button onClick={onShareWhatsApp} style={{ padding: "8px 10px", borderRadius: 8, border: "1px solid #e5e7eb", background:"#fff", cursor:"pointer" }}>WhatsApp</button>
          </div>
        </ImageWrap>

        <Info>
          <Title>{product.title}</Title>
          <Price>₹{Number(product.price).toLocaleString("en-IN")}</Price>
          <Stock inStock={inStock}>{inStock ? "In stock" : "Out of stock"}</Stock>
          <Desc>{product.description || product.desc || "No description available."}</Desc>

          <div style={{ display: "flex", gap: 12, alignItems: "center", marginTop: 8 }}>
            {avgRating && <div style={{ fontWeight: 700 }}>{avgRating} ★</div>}
            <div style={{ color: "#6b7280" }}>{reviews.length} reviews</div>
          </div>

          <div style={{ marginTop: 12 }}>
            <div style={{ marginBottom: 6, color: "#374151", fontWeight: 600 }}>Color</div>
            <VariantWrap>
              {colors.map(c => <ColorSwatch key={c} color={c} active={selectedColor===c} onClick={()=>setSelectedColor(c)} whileHover={{ scale:1.06 }} />)}
            </VariantWrap>

            <div style={{ marginTop:12, marginBottom:6, color:"#374151", fontWeight:600 }}>Size</div>
            <VariantWrap>
              {sizes.map(s => <SizeBtn key={s} active={selectedSize===s} onClick={()=>setSelectedSize(s)}>{s}</SizeBtn>)}
            </VariantWrap>
          </div>

          <Controls>
            <QtyBox>
              <QtyBtn aria-label="Decrease" onClick={decrement}>−</QtyBtn>
              <QtyVal>{qty}</QtyVal>
              <QtyBtn aria-label="Increase" onClick={increment}>+</QtyBtn>
            </QtyBox>

            <AddCartButton whileHover={{ scale: 1.03 }} onClick={handleAddToCart} disabled={!inStock || adding}>
              {adding ? "Adding..." : "Add to cart"}
            </AddCartButton>

            <button onClick={() => { toggleWishlist(); }} style={{ padding: "10px 12px", borderRadius: 8, border: "1px solid #e5e7eb", background:"#fff", cursor:"pointer" }}>
              {inWishlist() ? "♥ Wishlisted" : "♡ Wishlist"}
            </button>
          </Controls>

          {selectedColor && selectedSize && (
            <SelectedInfo style={{ marginTop: 10 }}>Selected: <b>{selectedSize}</b> • <span style={{ color: selectedColor }}>Color</span></SelectedInfo>
          )}

          <ReviewsWrap>
            <div style={{ fontWeight:700, marginBottom:8 }}>Customer reviews</div>
            <form onSubmit={submitReview} style={{ display:"grid", gap:8, marginBottom:12 }}>
              <input ref={nameInputRef} type="text" placeholder="Your name" value={reviewForm.name} onChange={e=>setReviewForm({...reviewForm, name:e.target.value})} style={{ padding:8, borderRadius:6, border:"1px solid #e5e7eb" }} />
              <select value={reviewForm.rating} onChange={e=>setReviewForm({...reviewForm, rating:Number(e.target.value)})} style={{ padding:8, borderRadius:6, border:"1px solid #e5e7eb" }}>
                <option value={5}>5 - Excellent</option>
                <option value={4}>4 - Good</option>
                <option value={3}>3 - Okay</option>
                <option value={2}>2 - Poor</option>
                <option value={1}>1 - Terrible</option>
              </select>
              <textarea placeholder="Write your review" value={reviewForm.text} onChange={e=>setReviewForm({...reviewForm, text:e.target.value})} rows={3} style={{ padding:8, borderRadius:6, border:"1px solid #e5e7eb" }} />

              <div style={{ display:"flex", gap:8 }}>
                <button type="submit" disabled={submittingReview} style={{ padding:"10px 12px", borderRadius:8, background:"#111827", color:"#fff", border:"none", cursor:"pointer" }}>
                  {submittingReview ? "Submitting..." : "Submit review"}
                </button>

                <button type="button" onClick={resetReviewForm} disabled={!reviewForm.name && !reviewForm.text && reviewForm.rating === 5} style={{ padding:"10px 12px", borderRadius:8, background:"#fff", border:"1px solid #e5e7eb", cursor:"pointer" }}>
                  Reset
                </button>
              </div>
            </form>

            {(displayedReviews || []).length === 0 && <div>No reviews yet.</div>}
            {(displayedReviews || []).map(r => (
              <div key={r.id} style={{ marginBottom:10 }}>
                <div style={{ fontWeight:700 }}>{r.name} <span style={{ color:"#f59e0b", marginLeft:8 }}>{'★'.repeat(Math.max(1, Math.min(5, Number(r.rating)||5)))}</span></div>
                <div style={{ color:"#374151" }}>{r.text}</div>
              </div>
            ))}

            {reviews.length > displayedReviews.length && (
              <div style={{ marginTop:8 }}>
                <button onClick={()=>setReviewPageSize(s=>s+5)} style={{ padding:"8px 10px", borderRadius:6, border:"1px solid #e5e7eb", background:"#fff", cursor:"pointer" }}>
                  Load more reviews
                </button>
              </div>
            )}
          </ReviewsWrap>

          <div style={{ marginTop: 28 }}>
            <div style={{ fontWeight:700 }}>Related products</div>
            <RelatedGrid>
              {related.length ? related.map(p => (
                <RelatedCard key={p.id} to={`/product/p${p.id}`}>
                  <RelatedImg src={p.img} alt={p.title} />
                  <div style={{ marginTop:8, fontWeight:600 }}>{p.title}</div>
                  <div style={{ color:"#6b7280", marginTop:6 }}>₹{Number(p.price).toLocaleString()}</div>
                </RelatedCard>
              )) : <div style={{ marginTop:8, color:"#6b7280" }}>No related products found.</div>}
            </RelatedGrid>
          </div>

          {recent && recent.length > 0 && (
            <div style={{ marginTop:18 }}>
              <div style={{ fontWeight:700 }}>Recently viewed</div>
              <RVList>
                {recent.map(r => (
                  <Link key={r.id} to={`/product/p${r.id}`}>
                    <img src={r.image || r.img} alt={r.title} />
                    <div style={{ fontSize:13, marginTop:8, fontWeight:600 }}>{r.title}</div>
                    <div style={{ color:"#0b74de", marginTop:6 }}>₹{Number(r.price).toLocaleString()}</div>
                  </Link>
                ))}
              </RVList>
            </div>
          )}

        </Info>
      </Container>

      <AnimatePresence>
        {toast && (
          <Toast initial={{ opacity:0, y:18 }} animate={{ opacity:1, y:0 }} exit={{ opacity:0, y:18 }} style={{ background: toast.type === "warn" ? "#4b2500" : "#111827" }}>
            {toast.text}
          </Toast>
        )}
      </AnimatePresence>

      <AnimatePresence>
        {zoomOpen && (
          <motion.div initial={{ opacity:0 }} animate={{ opacity:1 }} exit={{ opacity:0 }} onClick={()=>setZoomOpen(false)} style={{ position:"fixed", inset:0, background:"rgba(2,6,23,0.6)", display:"flex", alignItems:"center", justifyContent:"center", zIndex:100000 }}>
            <img src={mainImg || product.image || product.img} alt={product.title} style={{ maxWidth:"92vw", maxHeight:"92vh", borderRadius:10, objectFit:"contain", background:"#fff", padding:8 }} />
          </motion.div>
        )}
      </AnimatePresence>

      {showSticky && (
        <StickyBar>
          <div style={{ flex:1, textAlign:"left" }}>
            <div style={{ fontWeight:700 }}>{product.title}</div>
            <div style={{ color:"#6b7280" }}>₹{Number(product.price).toLocaleString()}</div>
          </div>
          <div style={{ display:"flex", gap:8 }}>
            <button onClick={handleAddToCart} style={{ padding:"10px 14px", borderRadius:8, background:"#111827", color:"#fff", border:"none" }}>{adding ? "Adding..." : "Add to cart"}</button>
            <button onClick={() => window.scrollTo({ top: 0, behavior: "smooth" })} style={{ padding:"10px 12px", borderRadius:8, background:"#fff", border:"1px solid #e5e7eb" }}>Top</button>
          </div>
        </StickyBar>
      )}
    </PageWrapper>
  );
};

export default ProductDetail;
